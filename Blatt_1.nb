Cell[BoxData[
 RowBox[{
  RowBox[{"MyPrimes", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"j", ",", "samples", ",", " ", "multiples", ",", 
      RowBox[{"i", "=", "1"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"j", "=", 
      RowBox[{"N", "[", 
       RowBox[{"Sqrt", "[", "n", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"samples", "=", 
      RowBox[{"Range", "[", 
       RowBox[{"2", ",", "n"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"samples", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "\[LessEqual]", "j"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"multiples", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"k", "*", 
            RowBox[{"samples", "[", 
             RowBox[{"[", "i", "]"}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", 
             RowBox[{"samples", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", 
             RowBox[{"Floor", "[", 
              RowBox[{"n", "/", 
               RowBox[{"samples", "[", 
                RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], "}"}]}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"samples", "=", 
         RowBox[{"Complement", "[", 
          RowBox[{"samples", ",", "multiples"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"i", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "samples", "]"}]}]}], "]"}]}]], "Input",Expression\
UUID->"05255668-7aaf-445b-bba5-e613f336bc5a"],

Cell[BoxData[
 RowBox[{
  RowBox[{"MathPrimes", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"x", "=", "1"}], ",", " ", 
      RowBox[{"primes", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"NextPrime", "[", "x", "]"}], "\[LessEqual]", " ", "n"}], ",",
        "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"x", "=", 
         RowBox[{"NextPrime", "[", "x", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"primes", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"primes", ",", " ", "x"}], "]"}]}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "primes", "]"}]}]}], "]"}]}]], "Input",ExpressionU\
UID->"04729fb0-e888-4cdf-ac44-dee45a20c368"],

